print('2nd program')
print(9.99 > 9.98, 1000 != 1000.1)
print('3rd program')
print((1234 % 1000 // 10) + (5678 % 1000 // 10))
print('4th program')
print(((int(13.42 * 100 % 100)) == (int(42.13 * 100 // 100))) or ((int(13.42 * 100 // 100)) == int(42.13 * 100 % 100)))
